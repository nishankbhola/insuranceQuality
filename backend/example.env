# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
ENABLE_GEMINI_ANALYSIS=true  # Set to false to disable Gemini analysis and save API calls
GEMINI_DAILY_LIMIT=45  # Conservative limit for free tier (50 calls/day)

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True

# Application Configuration
UPLOAD_FOLDER=uploads
MAX_CONTENT_LENGTH=16777216  # 16MB max file size
